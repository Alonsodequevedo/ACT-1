#include<stdio.h>

int main(){

	int opcion;
	while(1==1)
	{
	printf("que caso le gustaria realizar\n");
	
	scanf("%d", &opcion);
	switch(opcion)
	{
		case 1:
			printf("Puebla\n");
		break;
		
		case 2:
			printf("Merida\n");
		break;
		
		case 3:
			printf("Queretaro\n");
		break;	
		
		case 4:
			printf("Monterrey\n");
		break;
		
		case 5:
			printf("Cancun\n");
		break;
		
		case 6:
			printf("Acapulco\n");
		break;
	} 
}
	return 0;
}
